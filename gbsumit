local char = plr.Character or plr.CharacterAdded:Wait()
local hrp = char:WaitForChild("HumanoidRootPart")
local running = false
local flying = false

local function updatePos()
    local pos = hrp.Position
    xVal.Text = tostring(math.floor(pos.X))
    yVal.Text = tostring(math.floor(pos.Y))
    zVal.Text = tostring(math.floor(pos.Z))
end

copyBtn.MouseButton1Click:Connect(function()
    local pos = hrp.Position
    local str = tostring(math.floor(pos.X)) .. "," .. tostring(math.floor(pos.Y)) .. "," .. tostring(math.floor(pos.Z))
    if setclipboard then
        setclipboard(str)
        copyBtn.Text = "Copied!"
        wait(1)
        copyBtn.Text = "Copy Pos"
    end
end)

mode1Btn.MouseButton1Click:Connect(function()
    currentMode = "JUST_TP"
    mode1Btn.Text = "● Just TP"
    mode1Frame.BackgroundColor3 = Color3.new(0.18, 0.24, 0.18)
    mode2Btn.Text = "○ Fly Fast"
    mode2Frame.BackgroundColor3 = Color3.new(0.14, 0.14, 0.2)
    mode3Btn.Text = "○ Fly Slow"
    mode3Frame.BackgroundColor3 = Color3.new(0.14, 0.14, 0.2)
end)

mode2Btn.MouseButton1Click:Connect(function()
    currentMode = "FLY_FAST"
    mode1Btn.Text = "○ Just TP"
    mode1Frame.BackgroundColor3 = Color3.new(0.14, 0.14, 0.2)
    mode2Btn.Text = "● Fly Fast"
    mode2Frame.BackgroundColor3 = Color3.new(0.18, 0.24, 0.18)
    mode3Btn.Text = "○ Fly Slow"
    mode3Frame.BackgroundColor3 = Color3.new(0.14, 0.14, 0.2)
end)

mode3Btn.MouseButton1Click:Connect(function()
    currentMode = "FLY_SLOW"
    mode1Btn.Text = "○ Just TP"
    mode1Frame.BackgroundColor3 = Color3.new(0.14, 0.14, 0.2)
    mode2Btn.Text = "○ Fly Fast"
    mode2Frame.BackgroundColor3 = Color3.new(0.14, 0.14, 0.2)
    mode3Btn.Text = "● Fly Slow"
    mode3Frame.BackgroundColor3 = Color3.new(0.18, 0.24, 0.18)
end)

local function parseCoords(text)
    local nums = {}
    for num in string.gmatch(text, "%-?%d+") do
        table.insert(nums, tonumber(num))
    end
    if #nums >= 3 then
        return Vector3.new(nums[1], nums[2], nums[3])
    end
    return nil
end

local function goTo(pos)
    if currentMode == "JUST_TP" then
        hrp.CFrame = CFrame.new(pos)
    elseif currentMode == "FLY_FAST" then
        flying = true
        local bv = Instance.new("BodyVelocity")
        bv.MaxForce = Vector3.new(4000, 4000, 4000)
        bv.Velocity = Vector3.new(0, 0, 0)
        bv.Parent = hrp
        
        while flying and (hrp.Position - pos).Magnitude > 8 do
            local dir = (pos - hrp.Position).Unit
            bv.Velocity = dir * 80
            wait(0.1)
        end
        
        bv:Destroy()
        flying = false
        hrp.CFrame = CFrame.new(pos)
    elseif currentMode == "FLY_SLOW" then
        flying = true
        local bv = Instance.new("BodyVelocity")
        bv.MaxForce = Vector3.new(4000, 4000, 4000)
        bv.Velocity = Vector3.new(0, 0, 0)
        bv.Parent = hrp
        
        while flying and (hrp.Position - pos).Magnitude > 8 do
            local dir = (pos - hrp.Position).Unit
            bv.Velocity = dir * 25
            wait(0.1)
        end
        
        bv:Destroy()
        flying = false
        hrp.CFrame = CFrame.new(pos)
    end
end

local coords = {coord1, coord2, coord3, coord4, coord5, coord6, coord7, coord8}

startBtn.MouseButton1Click:Connect(function()
    if running then return end
    running = true
    startBtn.Text = "Running..."
    
    spawn(function()
        for i = 1, #coords do
            if not running then break end
            
            local box = coords[i]
            if box.Text ~= "" then
                local targetPos = parseCoords(box.Text)
                if targetPos then
                    startBtn.Text = "Going " .. i
                    goTo(targetPos)
                    
                    while (hrp.Position - targetPos).Magnitude > 10 and running do
                        wait(0.5)
                    end
                    
                    startBtn.Text = "Reached " .. i
                    wait(1)
                end
            end
        end
        
        if running then
            startBtn.Text = "Complete!"
            wait(2)
            startBtn.Text = "Start"
            running = false
        end
    end)
end)

stopBtn.MouseButton1Click:Connect(function()
    running = false
    flying = false
    startBtn.Text = "Start"
end)

spawn(function()
    while true do
        updatePos()
        wait(1)
    end
end)
